# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
	. ~/.bashrc
fi

# User specific environment and startup programs

PATH=$PATH:$HOME/.local/bin:$HOME/bin

export PATH
export LANG=ko_KR.UTF-8
export LANGUAGE=ko_KR.:ko
export LC_ALL=ko_KR.UTF-8
